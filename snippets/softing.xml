  <!-- softings one build script for possible automations
       We have to be synchronised in inner part of this entry ...
  -->
  <remove-project name="LineageOS/android_build" />
  <project path="build/make" name="VCPI.android.build" groups="softing,pdk" remote="host" revision="refs/heads/vcpi-a13-main" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
    <linkfile src="softing-build.sh" dest="softing-build.sh" />
  </project>

  <!-- Also here: we have to synchonise the inner part of this entry -->
  <remove-project name="LineageOS/android_build_soong" />
  <project path="build/soong" name="VCPI.android.build.soong" groups="softing,pdk,tradefed" remote="host" revision="refs/heads/vcpi-a13-main" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- remove all darwin/macos related projects -->
  <remove-project name="platform/prebuilts/clang/host/darwin-x86" />
  <remove-project name="platform/prebuilts/go/darwin-x86" />

